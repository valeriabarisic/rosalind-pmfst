      "source": [
        "\n",
        "def Count(t,p):\n",
        "  count=0\n",
        "  l=len(t)\n",
        "  k=len(p)\n",
        "  for i in range(0,l-k+1):\n",
        "    if (t[i:i+k]==p):\n",
        "      count=count+1\n",
        "  return count\n"
      ]
    },
